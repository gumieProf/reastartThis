{
	"name": "Restart this",
	"version": "1.0.0",
	"description": "You can easily start your own Youtube video!",
	"manifest_version": 3,
	"minimum_chrome_version": "100",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
	"tabs",
    "activeTab",
    "contextMenus",
	"storage"
	],
	"options_page": "option.html",
	"content_scripts": [
		{
			"matches": ["https://*.youtube.com/*"],
			"js": [ "content.js" ]
		}
	]
}
